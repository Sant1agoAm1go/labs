#include <stdio.h>
#include <math.h>
double my_exp (double  x, int n){
	double s = 1, p = 1;
        double y = x*x;
	for(int i = 2; i < n+; i++) {
		p = -p*y/i;
		s+=p;
	}
	return s;
}
int main() {
	double x;
	int n;
	printf("Введите степень x и точность n: ");
	scanf("%lf",&x);
	scanf("%d",&n);
	double s = my_exp(x,n);
	printf("%lf\n",s);
	printf("%lf\n",exp(-x*x));
	return 0;
}
